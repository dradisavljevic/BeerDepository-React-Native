{
  "name": "beerdepository",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "iphone8": "react-native run-ios --simulator=\"iPhone 8\"",
    "build:android:debug": "cd ./android && ./gradlew app:assembleDebug && ./gradlew installDebug && adb reverse tcp:8081 tcp:8081",
    "prettify": "lint-staged",
    "clean": "rm -rf node_modules/ && npm install",
    "tron": "node_modules/.bin/reactotron",
    "remove-haste": "rm -rf /tmp/hast-map-react-native-packager-*",
    "droid-reverse-port": "adb reverse tcp:9090 tcp:9090",
    "rn-tron-droid": "npm run android && npm run droid-reverse-port",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "iphone14": "react-native run-ios --simulator=\"iPhone 14\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "git update-index --again"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@react-native-community/netinfo": "^9.3.7",
    "@react-navigation/native": "^6.0.16",
    "@react-navigation/native-stack": "^6.9.4",
    "axios": "^1.2.1",
    "prettier": "^2.8.0",
    "react": "17.0.2",
    "react-native": "0.68.2",
    "react-native-fast-image": "^8.6.3",
    "react-native-gesture-handler": "^2.8.0",
    "react-native-image-zoom-viewer": "^3.0.1",
    "react-native-localization": "^2.3.1",
    "react-native-safe-area-context": "^4.4.1",
    "react-native-screens": "^3.18.2",
    "react-native-splash-screen": "^3.3.0",
    "react-native-vector-icons": "^9.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/react": "^18.0.26",
    "@types/react-native": "^0.70.8",
    "babel-jest": "^26.6.3",
    "babel-plugin-transform-inline-environment-variables": "^0.4.4",
    "eslint": "^7.32.0",
    "husky": "^8.0.2",
    "jest": "^26.6.3",
    "lint-staged": "^13.1.0",
    "metro-react-native-babel-preset": "^0.67.0",
    "react-test-renderer": "17.0.2",
    "tslint": "^6.1.3",
    "typescript": "^4.9.3"
  },
  "jest": {
    "preset": "react-native"
  }
}
